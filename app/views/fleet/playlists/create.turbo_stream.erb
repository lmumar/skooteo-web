<%= turbo_stream.prepend "playlists" do %>
  <%= render @playlist.becomes(Playlist) %>
<% end %>

<%= turbo_stream.update "modal-container" do %>
  <%= render partial: 'edit' %>
<% end %>

<%= turbo_stream.replace "save_notification" do %>
  <%= render partial: 'shared/save_notification', object: @playlist, as: :record %>
<% end %>
